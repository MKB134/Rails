<header class="header-container">
  <div class="col-8">
    <h1><%= link_to 'Test-Guru', root_path %></h1>
  </div>
  <div class="col-4">
    <% if logged_in? %>
      Welcome, <b><%= current_user.email %></b> Guru
      <%= link_to 'Logout', logout_path %>
    <% else %>
    <%= link_to 'Signup', signup_path %>
    <%= link_to 'Login', login_path %>
   <% end %>
  </div>
</header>
