
<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td><%= test.questions.count %></td>
  <td><%= button_to t('.start_button'), start_test_path(test), class: 'btn btn-primary' %></td>
  <td><%= link_to t('.show_button'), admin_test_path(test), class: 'btn btn-secondary' %></td>
  <td><%= link_to t('.edit_button'), edit_admin_test_path(test),class: 'btn btn-info' %></td>
  <td>
    <%= link_to t('.delete_button'),
        admin_test_path(test),
        method: :delete,
        data: { confirm:'Are you sure' }, class: 'btn btn-danger' %>
  </td>
</tr>
